<div class="row">
  <button phx-click="new">Log new feed</button>
</div>

<table>
  <thead>
    <tr>
      <th>Past 24 hours</th>
    </tr>
  </thead>
  <tbody id="feeds">
    <%= for feed <- @feeds do %>
      <tr id="feed-<%= feed.id %>">
        <td><%= feed.started_at |> relative_datetime %></td>
      </tr>
    <% end %>
  </tbody>
</table>
